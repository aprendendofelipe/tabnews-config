{
  "name": "@tabnews/config",
  "version": "0.0.1",
  "description": "Configurações padronizadas dos projetos TabNews",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./eslint": "./src/shared/eslint.config.cjs",
    "./prettier": "./src/shared/prettier.config.cjs"
  },
  "scripts": {
    "lint": "eslint . && prettier --check .",
    "format": "eslint --fix . && prettier --write .",
    "commit": "cz",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@eslint/js": "9.9.0",
    "@next/eslint-plugin-next": "14.2.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-primer-react": "5.4.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-vitest": "0.5.4",
    "eslint-plugin-vitest-globals": "1.5.0",
    "globals": "15.9.0",
    "prettier": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@tabnews/config": "file:.",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.9"
  },
  "author": "Turma do TabNews",
  "repository": "aprendendofelipe/tabnews-config",
  "keywords": [
    "cli",
    "config",
    "tabnews"
  ]
}
